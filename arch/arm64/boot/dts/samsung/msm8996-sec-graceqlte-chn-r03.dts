/dts-v1/;

#include "../qcom/msm8996-v3.dtsi"
#include "../qcom/msm8996-pm8994-pm8004.dtsi"
#include "../qcom/msm8996-mtp.dtsi"
#include "msm8996-max77838.dtsi"
#include "msm8996-s2mpb02-grace.dtsi"
#include "msm8996-sec-common.dtsi"
#include "msm8996-sec-graceqlte-r03.dtsi"
#include "msm8996-sec-graceqlte-chn-battery-r00.dtsi"

/{
	reserved-memory {
		secure_camera_region: secure_camera_region {
			compatible = "shared-dma-pool";
			no-map;
			alloc-ranges = <0 0x00000000 0 0xffffffff>;
			alignment = <0 0x200000>;
			size = <0 0x1C00000>;
			label = "secure_camera_region";
		};

		secure_camera_scratch_region: secure_camera_scratch_region {
			compatible = "shared-dma-pool";
			no-map;
			alloc-ranges = <0 0x00000000 0 0xffffffff>;
			alignment = <0 0x200000>;
			size = <0 0x200000>;
			label = "secure_camera_scratch_region";
		};
	};
};

&removed_regions {
	reg = <0 0x85800000 0 0x3000000>;
};

&qcom_seecom {
	reg = <0x86600000 0x2200000>;
};

&soc {
	qcom,ion {
		qcom,ion-heap@17 { /* SECURE CAMERA HEAP */
			reg = <17>;
			memory-region = <&secure_camera_region>;
			qcom,ion-heap-type = "HYP_CMA";
		};

		qcom,ion-heap@18 { /* SECURE CAMERA HEAP */
			reg = <18>;
			memory-region = <&secure_camera_scratch_region>;
			qcom,ion-heap-type = "HYP_CMA";
		};
	};
};

&slim_msm {
	imp_list: imp_list {
		#list-imp-cells = <3>;
	};

	tasha_codec {
		imp-table = <&imp_list 0 13 0>,
				 <&imp_list 14 42 5>,
				 <&imp_list 43 100 6>,
				 <&imp_list 101 200 7>,
				 <&imp_list 201 450 8>,
				 <&imp_list 451 1000 8>,
				 <&imp_list 1001 0x7fffffff 5>;
	};
};

/ {
	model = "Samsung GRACEQ LTE CHN PROJECT Rev03";
	compatible = "qcom,msm8996-mtp", "qcom,msm8996", "qcom,mtp";
	qcom,board-id = <8 3>;

	aliases {
		i2c1 = &i2c_1;
		uart1 = &blsp1_uart2;
		uart0 = &blsp2_uart1;
		i2c8 = &i2c_8;
		i2c18 = &i2c_18;
		i2c23 = &i2c_23;
		spi0 = &spi_0;
		spi6 = &spi_6;
		spi10 = &spi_10;
		i2c2 = &i2c_2;
		spi11 = &spi_11;
		i2c9 = &i2c_9;
	};
};
